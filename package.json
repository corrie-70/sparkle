{
  "name": "sparkle",
  "version": "0.1.0",
  "private": true,
  "main": "./build/main.js",
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.9.3",
    "babel-plugin-import": "^1.13.3",
    "customize-cra": "^1.0.0",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.9",
    "less": "^3.13.0",
    "less-loader": "^7.1.0",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "electron-dev": "npm run start-main && electron .",
    "pack-win64": "electron-builder --win --x64",
    "start-main": "node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.electron.js"
