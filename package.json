  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1",
    "ts-loader": "^8.0.12",
    "webpack-cli": "^4.2.0"
  },
  "author": {
    "name": "corrie",
    "email": "kehuang1013@gmail.com",
    "url": "https://github.com/corrie-70"
  },
  "build": {
    "productName": "sparkle",
    "appId": "com.corrie.sparkle",
    "directories": {
      "output": "pack"
    },
    "files": [
      "build/**/*"
    ],
    "extraMetadata": {
      "main": "build/main.js"
    },
    "publish": [
      {
        "provider": "generic",
        "url": ""
      }
    ]
  }
